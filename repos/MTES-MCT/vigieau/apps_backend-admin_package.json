  "scripts": {
    "build": "nest build",
    "scalingo-postbuild": "nest build && cp -r src/mail_templates/*.hbs dist/apps/backend-admin/src/mail_templates/ && mkdir -p dist/apps/backend-admin/src/mail_templates/partials && cp -r src/mail_templates/partials/*.hbs dist/apps/backend-admin/src/mail_templates/partials/ && git clone https://github.com/felt/tippecanoe.git && make -j --directory=tippecanoe/ && sed -i 's/PREFIX ?= \\/usr\\/local/PREFIX ?= ..\\/tippecanoe_program/g' tippecanoe/Makefile && make install --directory=tippecanoe/ && rm -rf tippecanoe/",
    "postinstall": "cd ../../ && yarn install",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:coverage": "nyc --silent nest start --watch --max-old-space-size=4096",
    "start:debug": "nest start --debug --watch --max-old-space-size=4096",
    "start:prod": "node dist/apps/backend-admin/src/main --max-old-space-size=4096",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "typeorm migration:create",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
