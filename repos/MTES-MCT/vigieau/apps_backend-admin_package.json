    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.2.0",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.5.0",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
