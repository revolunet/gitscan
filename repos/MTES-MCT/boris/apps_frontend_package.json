{
  "name": "@boris/frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.19.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "start:dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "start": "node build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "lighthouse-mobile": "lhci autorun --config=./.lighthouse-mobile.json",
    "lighthouse-desktop": "lhci autorun --config=./.lighthouse-desktop.json",
    "generate-api-types": "openapi-typescript http://localhost:3000/api/documentation/schema/json --output src/lib/utils/generated-api-types.ts",
    "generate-address-datagouv-types": "openapi-typescript https://data.geopf.fr/geocodage/openapi.yaml --output src/lib/utils/generated-address-datagouv-types.ts"
  },
  "dependencies": {
    "@sentry/sveltekit": "^8.42.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@gouvfr/dsfr": "^1.14.2",
    "@gouvfr/dsfr-chart": "^2.0.4",
    "@keyv/serialize": "^1.1.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/leaflet.markercluster": "^1.5.5",
    "@zerodevx/svelte-img": "^2.1.2",
    "cssnano": "^7.0.6",
    "eslint-plugin-svelte": "^2.36.0",
    "html2pdf.js": "^0.12.1",
    "keyv": "^5.5.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "nanoid": "^5.0.8",
    "openapi-typescript": "^7.8.0",
    "postcss": "^8.4.49",
    "postcss-custom-media": "^11.0.5",
    "prettier-plugin-svelte": "^3.1.2",
    "super-sitemap": "^1.0.3",
    "svelte": "^5.16.6",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "vite": "^5.0.3",
    "vitest": "^2.0.0",
    "zod": "^4.1.11"
  },
  "type": "module"
}
