{
  "name": "roadmaps-faciles",
  "description": "Roadmaps et product feedbacks, facilement.",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "scripts": {
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons && yarn prisma generate",
    "dev": "next dev --turbopack",
    "build": "next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && cp -r src/generated/prisma/*.node .next/standalone/node_modules/.prisma/client",
    "postbuild": "next-sitemap",
    "start": "node .next/standalone/server.js",
    "lint": "next typegen && eslint",
    "export": "next export",
    "build:export": "npm run build && npm run export",
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "generateEnvDeclaration": "yarn run-script scripts/generateEnvDeclaration.ts",
    "seed": "prisma db seed",
    "prisma:studio": "prisma studio -b none",
    "prisma:reset": "prisma migrate reset --skip-seed --skip-generate"
  },
  "resolutions": {
    "estree-walker": "2.0.2"
  },
  "dependencies": {
