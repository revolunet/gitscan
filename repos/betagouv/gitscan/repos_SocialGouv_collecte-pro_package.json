{
  "name": "collectepro",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build-all": "npm run build-control-detail && npm run build-questionnaire-create && npm run build-questionnaire-detail && npm run build-session-management && npm run build-sidebar",
    "build-control-detail": "parcel build static/src/control-detail.js --dist-dir static/dist --public-url /static/dist --no-source-maps",
    "build-questionnaire-create": "parcel build static/src/questionnaire-create.js --dist-dir static/dist --public-url /static/dist --no-source-maps",
    "build-questionnaire-detail": "parcel build static/src/questionnaire-detail.js --dist-dir static/dist --public-url /static/dist --no-source-maps",
    "build-session-management": "parcel build static/src/session-management.js --dist-dir static/dist --public-url /static/dist --no-source-maps",
    "build-sidebar": "parcel build static/src/sidebar.js --dist-dir static/dist --public-url /static/dist --no-source-maps",
    "watch-control-detail": "parcel watch static/src/control-detail.js --dist-dir static/dist --public-url /static/dist",
    "watch-questionnaire-create": "parcel watch static/src/questionnaire-create.js --dist-dir static/dist --public-url /static/dist",
    "watch-questionnaire-detail": "parcel watch static/src/questionnaire-detail.js --dist-dir static/dist --public-url /static/dist",
    "watch-session-management": "parcel watch static/src/session-management.js --dist-dir static/dist --public-url /static/dist",
    "watch-sidebar": "parcel watch static/src/sidebar.js --dist-dir static/dist --public-url /static/dist",
    "postinstall": "npm run build-all",
    "test": "jest"
  },
  "targets": {
    "default": {
      "context": "browser",
      "includeNodeModules": true,
      "outputFormat": "esmodule"
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bootstrap": "^4.6.0",
    "core-js": "^3.46.0",
    "dropzone": "^5.9.3",
    "file-saver": "^2.0.5",
    "jquery": "^3.6.0",
    "jszip": "^3.7.1",
    "jszip-utils": "^0.1.0",
    "mitt": "^3.0.1",
    "regenerator-runtime": "^0.14.1",
    "report-validity": "^1.0.1",
    "store": "^2.0.12",
    "sweetalert2": "^11.26.3",
    "vue": "^3.5.24",
    "vue3-datepicker": "^0.4.0",
    "vue3-sidebar-menu": "^23.0.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@parcel/transformer-vue": "^2.12.0",
    "@types/jquery": "^3.5.33",
    "@vue/compiler-sfc": "^3.5.24",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "jest": "^29.7.0",
    "parcel": "^2.16.1",
    "process": "^0.11.10"
  }
}
