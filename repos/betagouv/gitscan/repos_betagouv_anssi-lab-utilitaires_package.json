{
  "name": "lab-anssi-utilitaires",
  "version": "1.0.0",
  "description": "Une application backend fournissant des utilitaires transverses pour le Lab de l'ANSSI",
  "main": "dist/serveur.js",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "start": "node --import tsx src/index.ts",
    "start:dev": "env $(cat .env) pnpm run start --watch",
    "test:watch": "pnpm run test:typecheck -- --watch & pnpm run test:logic",
    "test:typecheck": "tsc --noEmit",
    "test:logic": "vitest",
    "preinstall": "npx only-allow@1.2.2 pnpm"
  },
  "author": "ThibaudMZN",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "~4.17.21",
    "@types/supertest": "~6.0.3",
    "supertest": "~7.1.4",
    "typescript": "~5.2.2",
    "vitest": "~4.0.8"
  },
  "dependencies": {
    "express": "~4.22.1",
    "tsx": "~4.20.5",
    "yaml": "~2.8.0"
  }
}
