{
  "name": "@dsfr-kit/generator-core",
  "version": "0.0.1",
  "description": "Core logic and AST definitions for DSFR generator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "biome lint .",
    "format": "biome format ."
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@gouvfr/dsfr": "^1.14.2",
    "comment-parser": "^1.4.1",
    "node-html-parser": "^7.0.1",
    "tree-sitter": "^0.25.0",
    "tree-sitter-embedded-template": "^0.25.0",
    "tree-sitter-html": "^0.23.2",
    "tree-sitter-javascript": "^0.25.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^20.19.27",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
