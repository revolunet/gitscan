{
  "name": "info-medicament",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons && kysely migrate:latest && kysely seed run --specific 1725439927263_searchIndex",
    "test": "npm run test:unit",
    "test:unit": "vitest --exclude '**/*.integration.test.ts' --exclude '**/*.ui.test.tsx'",
    "test:integration": "vitest run 'integration.test.ts'",
    "test:ui": "vitest run 'ui.test.tsx'",
    "test:all": "vitest run",
    "test:watch": "vitest --watch",
    "db:sync-grist": "npx tsx scripts/syncWithGrist.ts",
    "db:migrate:latest": "npx kysely migrate:latest",
    "db:migrate:down": "npx kysely migrate:down",
    "db:update-resume": "npx tsx scripts/updateResumeData.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.16.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@gouvfr/dsfr-chart": "^1.0.0",
    "@hotjar/browser": "^1.0.9",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@sentry/nextjs": "^8.28.0",
    "@socialgouv/matomo-next": "^1.9.2",
    "axios": "^1.11.0",
    "csv-parse": "^5.5.6",
    "image-size": "^1.2.0",
    "jszip": "^3.10.1",
    "kysely": "^0.27.4",
    "mysql2": "^3.10.2",
    "next": "^15.5.10",
    "next-mdx-remote": "^5.0.0",
    "node-html-parser": "^7.0.1",
    "pg": "^8.12.0",
    "react": "^19.0.4",
    "react-dom": "^19.0.4",
    "react-spinners": "^0.17.0",
    "react-toastify": "^11.0.5",
    "sanitize-html": "^2.13.1",
    "sass": "^1.77.6",
    "server-cli-only": "^0.3.2",
    "server-only": "^0.0.1",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.15",
    "swr": "^2.2.5",
    "usehooks-ts": "^3.1.0",
    "windows-1252": "^3.0.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22",
    "@types/pg": "^8.11.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/sanitize-html": "^2.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9",
    "eslint-config-next": "^15.1.5",
    "eslint-config-prettier": "^10.0.1",
    "global-jsdom": "^26",
    "jsdom": "^26",
    "kysely-ctl": "^0.10.1",
    "nock": "^14.0.0-beta.15",
    "prettier": "^3.3.2",
    "typescript": "^5",
    "vitest": "^3.0.5"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}