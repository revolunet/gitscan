{
  "name": "analytics",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "db:new": "dbmate --env DATABASE_URL_ANALYTICS --migrations-table analytics_raw.schema_migrations --migrations-dir db/migrations new",
    "db:migrate": "dbmate --env DATABASE_URL_ANALYTICS --migrations-table analytics_raw.schema_migrations --migrations-dir db/migrations up",
    "build": "tsc",
    "job": "ts-node src/jobs/run-job.ts",
    "job:dist": "node dist/jobs/run-job.js",
    "prettier": "prettier --write . --plugin=prettier-plugin-organize-imports",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@sentry/node": "10.21.0",
    "dbmate": "2.28.0",
    "dotenv": "16.4.5",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "eslint-plugin-import": "2.32.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
