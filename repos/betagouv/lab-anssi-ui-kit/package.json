{
  "name": "@lab-anssi/ui-kit",
  "version": "1.42.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/lab-anssi-ui-kit.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betagouv/lab-anssi-ui-kit/issues"
  },
  "homepage": "https://github.com/betagouv/lab-anssi-ui-kit#readme",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "vite build && npm run prepack",
    "build:tokens": "node src/tokens/build.config.ts && pnpm prettier -w .storybook/lab-anssi-themes.css",
    "build:webcomponents": "vite -c vite.webcomponents.config.ts build",
    "build:styles:dsfr": "sass src/lib/styles/dsfr-variables.scss dist/assets/dsfr-variables.css -I node_modules -I node_modules/@gouvfr/dsfr --no-source-map",
    "prepare": "husky",
    "prepack": "npm run build:webcomponents && npm run copy:icons && publint",
    "test": "vitest run",
    "storybook:dev": "STORYBOOK_ENV=development storybook dev -p 6006",
    "storybook:build": "STORYBOOK_ENV=production storybook build",
    "storybook:build-dev": "STORYBOOK_ENV=development storybook build",
    "storybook:test": "STORYBOOK_ENV=production npm run test-storybook",
    "test-storybook": "vitest --project=storybook",
    "copy:icons": "cp -r node_modules/@gouvfr/dsfr/dist/icons/* src/lib/assets/icones/dsfr && cp -r src/lib/assets/ dist/",
    "preinstall": "npx only-allow@1.2.2 pnpm"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "type": "module",
  "exports": {
    "./dist/webcomponents": {
      "types": "./dist/webcomponents/lab-anssi-ui-kit.jsx.d.ts",
      "import": "./dist/webcomponents/lab-anssi-ui-kit.iife.js"
    }
  },
  "peerDependencies": {
    "svelte": "~5.38.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "~5.0.0",
    "@eslint/compat": "~1.4.1",
    "@eslint/js": "~9.39.2",
    "@fullhuman/postcss-purgecss": "~7.0.2",
    "@gouvfr/dsfr": "1.14.3",
    "@storybook/addon-a11y": "~10.1.11",
    "@storybook/addon-docs": "~10.1.11",
    "@storybook/addon-svelte-csf": "~5.0.10",
    "@storybook/addon-themes": "~10.1.11",
    "@storybook/addon-vitest": "~10.1.11",
    "@storybook/svelte-vite": "~10.1.11",
    "@sveltejs/vite-plugin-svelte": "~6.2.3",
    "@testing-library/jest-dom": "~6.9.1",
    "@testing-library/svelte": "~5.3.1",
    "@types/node": "~24.10.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "~9.2.1",
    "cssnano": "~7.1.2",
    "dotenv": "~17.2.3",
    "eslint": "~9.39.2",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-storybook": "~10.1.11",
    "eslint-plugin-svelte": "~3.13.1",
    "globals": "~16.5.0",
    "http-server": "~14.1.1",
    "husky": "~9.1.7",
    "lint-staged": "~16.2.7",
    "playwright": "~1.57.0",
    "postcss": "~8.5.6",
    "postcss-discard-empty": "~7.0.1",
    "postcss-root-to-host": "~0.0.0",
    "prettier": "~3.7.4",
    "prettier-plugin-svelte": "~3.4.1",
    "publint": "~0.3.16",
    "sass": "~1.97.2",
    "storybook": "~10.1.11",
    "style-dictionary": "^5.1.3",
    "svelte": "~5.46.1",
    "svelte-check": "~4.3.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.52.0",
    "vite": "~7.3.1",
    "vitest": "~3.2.4",
    "wait-on": "~9.0.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts,css,md,yml}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
