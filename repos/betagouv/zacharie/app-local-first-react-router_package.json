{
  "name": "zacharie-app-local-first-react-router",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "typecheck": "tsc -b",
    "dev": "prisma generate && vite --port 3234 --host",
    "dev-test": "VITE_HOST=127.0.0.1:3290 VITE_SCHEME=http VITE_TEST=true VITE_TEST_PLAYWRIGHT=true VITE_API_URL=http://localhost:3291 vite --port 3290",
    "vitest": "VITE_API_URL=http://localhost:3234 vitest",
    "build": "npm install && npm run prebuild && vite build && npx prisma generate",
    "lint": "eslint .",
    "preview": "vite preview --port 3234",
    "start": "NODE_ENV=production PORT=8080 node ./server-app.js",
    "start-local": "NODE_ENV=production PORT=3234 node ./server-app.js"
  },
  "dependencies": {
    "@af-utils/scrollend-polyfill": "^0.0.14",
    "@codegouvfr/react-dsfr": "1.28.0",
    "@e965/xlsx": "^0.20.3",
    "@prisma/client": "6.8.2",
    "@react-hookz/web": "^25.1.1",
    "@sentry/react": "^9.22.0",
    "@tailwindcss/vite": "^4.1.11",
    "@uidotdev/usehooks": "^2.4.1",
    "compression": "^1.8.1",
    "dayjs": "^1.11.13",
    "express": "^4.22.1",
    "fake-indexeddb": "^6.0.1",
    "idb-keyval": "^6.2.2",
    "p-queue": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-player": "^3.3.2",
    "react-router": "^7.12.0",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5",
    "sortablejs": "^1.15.6",
    "uuid": "^11.0.3",
    "zod": "^4.1.11",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@sentry/vite-plugin": "^2.22.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/compression": "^1",
    "@types/express": "^4",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "6.8.2",
    "prisma-types-export": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-pwa": "^0.21.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.2.4",
    "workbox-precaching": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 110,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
