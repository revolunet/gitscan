{
  "name": "mesquestionscyber",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json --threshold error && tsc -p tsconfig.node.json",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
