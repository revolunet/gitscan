    "@tsconfig/svelte": "~5.0.5",
    "@types/node": "~22",
    "eslint": "~9.22.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "~3.0.0",
    "globals": "~16.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "^3.4.1",
    "sass-embedded": "~1.93.2",
    "svelte": "~5.39.4",
    "svelte-check": "~4.3.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.49.0",
    "vite": "~7.2.7",
    "vitest": "~4.0.15"
