{
  "name": "maestro-server",
  "version": "0.0.1",
  "main": "./index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run migrate-latest && node --enable-source-maps --import @swc-node/register/esm-register --watch --watch-kill-signal SIGINT --inspect ./index.ts",
    "build": "tsc",
    "tsc": "tsc",
    "start": "npm run migrate-latest && node --enable-source-maps --import ./sentry.mjs --import @swc-node/register/esm-register ./index.ts",
    "test": "vitest",
    "test:integration": "vitest --project integration",
    "test:unit": "vitest --project unit",
    "lint": "eslint . --ext .js,.ts",
    "migrate-list": "NODE_OPTIONS='--enable-source-maps --import @swc-node/register/esm-register' knex migrate:list",
    "migrate-latest": "NODE_OPTIONS='--enable-source-maps --import @swc-node/register/esm-register' knex migrate:latest",
    "migrate-rollback-one": "NODE_OPTIONS='--enable-source-maps --import @swc-node/register/esm-register' knex migrate:down",
    "migrate-rollback-all": "NODE_OPTIONS='--enable-source-maps --import @swc-node/register/esm-register' knex migrate:rollback --all",
    "seed": "node --enable-source-maps --import @swc-node/register/esm-register database/seeds/dummy/index.ts",
    "seed:departments": "node --enable-source-maps --import @swc-node/register/esm-register database/seeds/departments/script.ts",
    "seed:laboratories": "node --enable-source-maps --import @swc-node/register/esm-register database/seeds/laboratories/script.ts",
    "ssd2-generate": "node --enable-source-maps --import @swc-node/register/esm-register ./scripts/updateSSD2Referential.ts",
    "check:emails": "node --enable-source-maps --import @swc-node/register/esm-register ./services/imapService/script.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@faker-js/faker": "^8.2.0",
    "@gouvfr/dsfr": "^1.13.0",
    "@sentry/node": "^10.32.1",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.1",
    "carbone": "^3.5.6",
    "compose-middleware": "^5.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^10.0.0",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^8.2.1",
    "fast-xml-parser": "^4.5.1",
    "handlebars": "^4.7.8",
    "helmet": "^5.0.2",
    "highland": "^2.13.5",
    "http-proxy-middleware": "^3.0.5",
    "imapflow": "^1.2.1",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "knex": "^3.1.0",
    "knex-stringcase": "^1.5.5",
    "kysely": "^0.27.4",
    "libxml2-wasm": "^0.6.0",
    "maestro-shared": "^0.0.1",
    "mailparser": "^3.9.1",
    "nodemailer": "^7.0.11",
    "openid-client": "^5.7.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "puppeteer-core": "^24.22.3",
    "sanitize-html": "^2.14.0",
    "ssh2-sftp-client": "^12.0.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@types/carbone": "^3.2.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.12",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.6",
    "@types/express-fileupload": "^1.4.1",
    "@types/highland": "^2.13.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mailparser": "^3.4.5",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4"
  }
}
