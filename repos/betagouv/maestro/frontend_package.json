{
  "name": "maestro-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cp -r ../node_modules/@codegouvfr ./node_modules/ && react-dsfr update-icons && vite",
    "start": "react-dsfr update-icons && vite",
    "tsc": "tsc",
    "build": "react-dsfr update-icons && tsc && vite build",
    "build-vite": "vite build",
    "scalingo-postbuild": "run-p build-vite build-storybook",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "AGPL-3.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@codegouvfr/react-dsfr": "^1.28.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.20",
    "@reduxjs/toolkit": "^2.4.0",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@types/node": "^20.11.30",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "clsx": "^2.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^10.1.10",
    "fetch-intercept": "^2.4.0",
    "i18next": "^23.10.0",
    "maestro-shared": "^0.0.1",
    "maplibre-gl": "^4.1.2",
    "openapi-explorer": "^2.2.734",
    "playwright": "^1.56.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^14.0.5",
    "react-map-gl": "^7.1.7",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.12.0",
    "sass": "^1.51.0",
    "storybook": "^10.1.10",
    "vite": "^7.1.12",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "whatwg-fetch": "^3.6.20",
    "workbox-webpack-plugin": "^7.1.0"
