{
  "name": "@les-communs/api",
  "version": "0.1.17",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "nest build && pnpm sentry:sourcemaps",
    "build:without-sourcemaps": "nest build",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "dev": "nest start --watch",
    "dev:staging": "NODE_ENV=staging nest start --watch",
    "start": "node dist/src/main.js",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "validate": "pnpm type-check && pnpm lint && pnpm format:check",
    "test": "pnpm test:unit && pnpm test:script",
    "test:unit": "jest --config test/jest.config.ts --runInBand --testPathIgnorePatterns='integration.spec.ts'",
    "test:integration": "jest --config test/jest.config.ts --runInBand --testPathPatterns='integration.spec.ts'",
    "test:e2e": "pnpm test:e2e:start && pnpm generate-types && jest --config test/jest-e2e.config.ts --runInBand --detectOpenHandles --no-buffer && pnpm test:e2e:stop",
    "test:e2e:start": "docker compose -f docker-compose.e2e-test.yml up -d",
    "test:e2e:stop": "docker compose -f docker-compose.e2e-test.yml down -v",
    "test:script": "jest --config test/jest.scripts.config.ts --runInBand",
    "test:api": "pnpm test:unit && pnpm test:integration",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e && pnpm test:script",
    "test:watch": "jest --config test/jest.config.ts --watch --runInBand --testPathIgnorePatterns='integration.spec.ts'",
    "test:watch:integration": "jest --config test/jest.config.ts --watch --runInBand --testPathPatterns='integration.spec.ts'",
    "test:cov": "jest --config test/jest.config.ts --coverage --testPathIgnorePatterns='integration.spec.ts'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config test/jest.config.ts --testPathIgnorePatterns='integration.spec.ts'",
    "test:ci": "pnpm test:unit && pnpm test:integration && pnpm test:e2e && pnpm test:script",
    "test:scripts:watch": "jest --config test/jest.scripts.config.ts --watch",
    "generate-types": "bash scripts/generate-types.sh",
    "convert-geo-api-to-openapi": "npx swagger2openapi https://geo.api.gouv.fr/definition.yml -o src/geo/openapi-definition.yml",
    "generate-geo-types": "openapi-typescript src/geo/openapi-definition.yml -o src/geo/api.d.ts",
    "seed:communes-and-epci:dev": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register scripts/seed-communes-and-epci/seed-communes-and-epci.ts",
    "seed:services": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register scripts/import-services-and-services-context/import-services-and-services-contexts.ts",
    "import:projects-tet": "cd scripts/import-projets-tet && cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register import-projets-tet.ts import-projets-tet-data.csv",
    "release": "commit-and-tag-version",
    "db:start": "docker compose up -d",
    "db:clean": "docker compose down -v",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push:pg",
    "db:migrate:drizzle": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:studio:staging": "cross-env NODE_ENV=staging drizzle-kit studio",
    "sentry:sourcemaps": "sentry-cli --url https://sentry.incubateur.net/ sourcemaps inject --org betagouv --project communs-de-la-transition-eco-api ./dist && sentry-cli --url https://sentry.incubateur.net/ sourcemaps upload --org betagouv --project communs-de-la-transition-eco-api ./dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@bull-board/nestjs": "^6.9.6",
    "@nestjs/bullmq": "^11.0.1",
    "@nestjs/common": "^11.0.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.20",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.20",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@sentry/cli": "^2.43.0",
    "@sentry/nestjs": "^10.28.0",
    "@sentry/node": "^10.27.0",
    "bullmq": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "http-proxy-middleware": "^3.0.3",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuidv7": "^1.0.2",
    "winston": "^3.17.0",
    "openapi-fetch": "^0.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.20",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.11.13",
    "@types/supertest": "^6.0.3",
    "commit-and-tag-version": "^12.5.1",
    "cross-env": "^10.1.0",
    "docker-compose": "^1.2.0",
    "jest": "^30.2.0",
    "openapi-typescript": "^7.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "testcontainers": "^11.7.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "npm": "use-pnpm-instead",
    "yarn": "use-pnpm-instead",
    "pnpm": "10.x",
    "node": "22.x"
  }
}
