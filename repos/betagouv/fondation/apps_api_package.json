{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "start": "dotenvx run -- nest start",
    "start:e2e": "dotenvx run -f .env.e2e -f .env -- nest start",
    "dev": "dotenvx run -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "types:check": "tsc --noEmit",
    "test": "jest",
    "test:it": "dotenvx run -- jest --runInBand --config ./test/jest/jest-integration.config.ts",
    "test:e2e": "dotenvx run -f .env.e2e -f .env -- jest --runInBand --config ./test/jest/jest-e2e.config.ts",
    "test:all": "dotenvx run -f .env.e2e -f .env -- jest --runInBand --config ./test/jest/jest-all.config.ts",
    "cli": "dotenvx run -- ts-node -r tsconfig-paths/register src/cli.ts",
    "prisma": "prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@dotenvx/dotenvx": "1.51.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@sentry/node": "^10.22.0",
    "@types/inquirer": "^8.2.12",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "exif-be-gone": "^1.5.1",
    "fast-xml-parser": "^5.3.0",
    "lodash": "^4.17.21",
    "nest-commander": "^3.20.1",
    "nestjs-zod": "^5.0.1",
    "node-xlsx": "^0.24.0",
    "pg": "^8.13.0",
    "pg-native": "^3.2.0",
    "prisma": "^6.19.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "shared-models": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@faker-js/faker": "catalog:",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "expect": "^30.2.0",
    "globals": "^16.2.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>/../"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(ts)$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(ts)"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
