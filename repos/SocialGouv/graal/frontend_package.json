{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --config eslint.config.mjs \"**/*.ts\" --fix && pnpm typecheck",
    "preview": "vite preview",
    "generate-types": "openapi-typescript http://localhost:8000/openapi.json -o src/types/api-generated.ts && eslint src/types/api-generated.ts --fix",
    "generate-types:dev": "openapi-typescript http://localhost:8000/openapi.json -o src/types/api-generated.ts && eslint src/types/api-generated.ts --fix",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.27.0",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^2.1.8",
    "chokidar": "^4.0.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write"
    ]
  }
}
