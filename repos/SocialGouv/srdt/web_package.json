{
  "name": "srdt-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "predev": "react-dsfr update-icons",
    "dev": "next dev --turbopack",
    "prebuild": "react-dsfr update-icons",
    "build": "next build",
    "build:standalone": "pnpm prebuild && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.16.10",
    "@sentry/nextjs": "^9.28.1",
    "@socialgouv/matomo-next": "^1.9.2",
    "@types/debounce-promise": "^3.1.9",
    "debounce-promise": "^3.1.2",
    "downshift": "^9.0.9",
    "jose": "^4.15.9",
    "next": "15.3.6",
    "next-auth": "^4.24.11",
    "postgres": "^3.4.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "sass": "^1.83.4",
    "typescript": "^5"
  },
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "onFail": "error"
    }
  }
}
