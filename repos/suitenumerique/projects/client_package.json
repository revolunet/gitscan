{
  "name": "projects",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "build": "react-app-rewired build",
    "lint": "eslint --ext js,jsx src config-overrides.js",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test",
    "test:acceptance": "cucumber-js --require tests/acceptance/cucumber.conf.js --require tests/acceptance/stepDefinitions/**/*.js --format @cucumber/pretty-formatter",
    "cunningham:generate": "cunningham -g css,scss,ts -o src",
    "patch-package": "patch-package"
  },
  "engines": {
    "node": "22"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "jest": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "presets": [
          "airbnb"
        ]
      },
      "requireConfigFile": false
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": "off",
      "no-unused-vars": "warn",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "src/setupTests.js",
            "**/*.test.js"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "singleQuote": true,
          "printWidth": 100,
          "trailingComma": "all"
        }
      ]
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/react": "^0.1.21",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gouvfr-lasuite/ui-kit": "^0.16.1",
    "@mdxeditor/editor": "^3.47.0",
    "@openfun/cunningham-react": "^3.2.3",
    "@radix-ui/react-popover": "^1.1.15",
    "classnames": "^2.5.1",
    "core-js-pure": "^3.45.1",
    "date-fns": "^2.30.0",
    "dequal": "^2.0.3",
    "history": "^5.3.0",
    "i18next": "24.2.2",
    "i18next-browser-languagedetector": "8.1.0",
    "initials": "^3.1.2",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.9",
    "patch-package": "^8.0.1",
    "photoswipe": "^5.4.4",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-app-rewired": "^2.2.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.5",
    "react-i18next": "15.4.1",
    "react-markdown": "^8.0.7",
    "react-photoswipe-gallery": "^2.2.7",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "react-textarea-autosize": "^8.5.5",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-orm": "^0.16.2",
    "redux-saga": "^1.3.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "reselect": "^4.1.8",
    "sails.io.js": "^1.2.1",
    "sass": "^1.87.0",
    "socket.io-client": "^2.5.0",
    "whatwg-fetch": "^3.6.20",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.49.0",
    "axios": "^1.6.2",
    "babel-preset-airbnb": "^5.0.0",
    "chai": "^4.5.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "playwright": "^1.49.0",
    "prettier": "^3.3.3",
    "string-replace-loader": "^3.2.0",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "@testing-library/react": "npm:@testing-library/react@^16.0.0"
  }
}
